def parity_checker() -> None:
    check = input ("What number do you want to check?")
    check = int(check)
    if check % 2 == 0:
        print ("Even")
    else:
        print ("Odd")
    # write your code here
    pass

parity_checker()
print("bla bla bla!!!! "*10)


